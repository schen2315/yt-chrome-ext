{
  "manifest_version": 2,

  "name": "youtube chrome extension",
  "description": "extension lets you section off a song on youtube for easy transcription",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "music-icon.png",
    "default_popup": "index.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://ixwyi5tpad.execute-api.us-east-1.amazonaws.com/dev/",
    "https://*/"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  }
}